    <PackageReference Include="Microsoft.TestPlatform.ObjectModel" Version="17.3.2" />
    <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="6.0.10" />
    <PackageReference Include="NUnit" Version="3.13.3" />
    <PackageReference Include="Selenium.InternetExplorer.WebDriver" Version="3.150.1" />
    <PackageReference Include="Selenium.WebDriver" Version="4.5.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="minimaltest.core">
      <HintPath>..\bin\minimaltest.core.dll</HintPath>
      <Private>True</Private>
    </Reference>
  </ItemGroup>

</Project>
