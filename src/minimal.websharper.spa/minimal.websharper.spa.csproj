<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <WebSharperProject>spa</WebSharperProject>
    <WebSharperBundleOutputDir>wwwroot/Content</WebSharperBundleOutputDir>
    <WebProjectOutputDir>$(MSBuildThisFileDirectory)/wwwroot</WebProjectOutputDir>
    <RootNamespace>minimal.websharper.spa</RootNamespace>
    <AssemblyName>minimal.websharper.spa</AssemblyName>
	<EnableUnsafeBinaryFormatterSerialization>true</EnableUnsafeBinaryFormatterSerialization>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="FSharp.Core" Version="6.0.5" />
    <PackageReference Include="Microsoft.TestPlatform.ObjectModel" Version="17.3.0" />
    <PackageReference Include="Selenium.InternetExplorer.WebDriver" Version="3.150.1" />
    <PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="104.0.5112.7900" />
    <PackageReference Include="Selenium.WebDriver.GeckoDriver" Version="0.31.0.1" />
    <PackageReference Include="Selenium.WebDriver.MSEdgeDriver" Version="104.0.1293.54" />
    <PackageReference Include="WebSharper" Version="6.0.0.228" />
    <PackageReference Include="WebSharper.CSharp" Version="6.0.0.228" />
    <PackageReference Include="WebSharper.UI" Version="6.0.0.230" />
    <PackageReference Include="WebSharper.UI.CSharp" Version="6.0.0.230" />
    <PackageReference Include="WebSharper.AspNetCore" Version="6.0.0.228" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\asplib.websharper\asplib.websharper.csproj" />
    <ProjectReference Include="..\iselenium.websharper\iselenium.websharper.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="minimaltest.websharper.spa">
      <HintPath>..\bin\minimaltest.websharper.spa.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties appsettings_1json__JsonSchema="" /></VisualStudio></ProjectExtensions>

</Project>