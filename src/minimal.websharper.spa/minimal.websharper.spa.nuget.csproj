<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <WebSharperProject>spa</WebSharperProject>
    <WebSharperBundleOutputDir>wwwroot/Content</WebSharperBundleOutputDir>
    <WebProjectOutputDir>$(MSBuildThisFileDirectory)/wwwroot</WebProjectOutputDir>
    <RootNamespace>minimal.websharper.spa</RootNamespace>
    <AssemblyName>minimal.websharper.spa</AssemblyName>
    <EnableUnsafeBinaryFormatterSerialization>true</EnableUnsafeBinaryFormatterSerialization>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="asplib.websharper" Version="0.2.0" />
    <PackageReference Include="iselenium.websharper" Version="0.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.10" />
    <PackageReference Include="NUnit" Version="3.13.3" />
    <PackageReference Include="NUnit.Engine" Version="3.15.2" />
    <PackageReference Include="Selenium.WebDriver" Version="4.7.0" />
    <PackageReference Include="FSharp.Core" Version="6.0.6" />
    <PackageReference Include="Microsoft.TestPlatform.ObjectModel" Version="17.3.2" />
    <PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="108.0.5359.7100" />
    <PackageReference Include="Selenium.WebDriver.GeckoDriver" Version="0.32.0" />
    <PackageReference Include="Selenium.WebDriver.MSEdgeDriver" Version="107.0.1418.42" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.5" />
    <PackageReference Include="WebSharper" Version="6.1.0.249" />
    <PackageReference Include="WebSharper.CSharp" Version="6.1.0.249" />
    <PackageReference Include="WebSharper.UI" Version="6.1.0.249" />
    <PackageReference Include="WebSharper.UI.CSharp" Version="6.1.0.249" />
    <PackageReference Include="WebSharper.AspNetCore" Version="6.1.0.249" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="minimaltest.websharper.spa">
      <HintPath>..\bin\minimaltest.websharper.spa.dll</HintPath>
      <Private>True</Private>
    </Reference>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties appsettings_1json__JsonSchema="" /></VisualStudio></ProjectExtensions>

</Project>