<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework>net6.0</TargetFramework>
    <RootNamespace>iselenium</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Toni Arnold</Authors>
    <Version>0.2.1</Version>
    <PackageId>iselenium.core</PackageId>
    <Product>aspnettest</Product>
    <Description>ASP.NET In-App-GUI-Tests with Selenium and NUnit</Description>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>Copyright Â© 2018-2022</Copyright>
    <PackageProjectUrl>https://github.com/toniarnold/aspnettest/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/toniarnold/aspnettest.git</RepositoryUrl>
    <PackageReleaseNotes>alpha</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.TestPlatform.ObjectModel" Version="17.3.2" />
    <PackageReference Include="NUnit.Engine" Version="3.15.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\asplib.core\asplib.core.csproj" />
    <ProjectReference Include="..\iselenium\iselenium.csproj" />
  </ItemGroup>


</Project>
