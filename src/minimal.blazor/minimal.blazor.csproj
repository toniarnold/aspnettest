<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
	<EnableUnsafeBinaryFormatterSerialization>true</EnableUnsafeBinaryFormatterSerialization>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="111.0.5563.1900-beta" />
    <PackageReference Include="Selenium.WebDriver.GeckoDriver" Version="0.32.2" />
    <PackageReference Include="Selenium.WebDriver.MSEdgeDriver" Version="110.0.1587.22-pre" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\asplib.blazor\asplib.blazor.csproj" />
    <ProjectReference Include="..\asplib.core\asplib.core.csproj" />
    <ProjectReference Include="..\asplib\asplib.csproj" />
    <ProjectReference Include="..\iselenium.blazor\iselenium.blazor.csproj" />
    <ProjectReference Include="..\iselenium.core\iselenium.core.csproj" />
    <ProjectReference Include="..\iselenium\iselenium.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="minimaltest.blazor">
      <HintPath>..\bin\minimaltest.blazor.dll</HintPath>
      <Private>True</Private>
    </Reference>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties appsettings_1json__JsonSchema="" /></VisualStudio></ProjectExtensions>

</Project>
