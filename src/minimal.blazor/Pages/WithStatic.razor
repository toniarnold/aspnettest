@page "/withstatic"
@inherits OwningComponentBase<Models.Main>
@inject Models.Main Main

<h1>minimalist test setup with static main</h1>

<input @bind="input" />
<button type="submit" id="SubmitButton" @onclick="Submit">Submit</button>
<ul>
    @foreach (var item in Main)
    {
        <li>@item</li>
    }
</ul>

@code {
    private String? input;

    private void Submit()
    {
        if (input != null)
        {
            Main.Add(input);
            input = null;
        }
    }
}
